{
  "name": "@samuelsson/eslint-config",
  "version": "2.4.0",
  "description": "eslint config for various javascript projects",
  "files": [
    "base.js",
    "react-typescript.js",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Erik Samuelsson",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./ --ext .js",
    "cm": "npx git-cz",
    "release": "yarn standard-version && git push --follow-tags origin main && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/samuelsson/eslint-config.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0"
  }
}
